{
  "name": "Hide LeetCode Difficulty",
  "version": "1.0",
  "description": "Hide the difficulty of a LeetCode Problem on the page",
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problemset/all/",
        "https://leetcode.com/problems/*"
      ],
      "js": ["index.js"]
    }
  ],
  "icons": { "32": "lc-32x32.png" },
  "manifest_version": 2
}
