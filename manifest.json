{
  "name": "Hide LeetCode Difficulty",
  "version": "1.0",
  "description": "Hide the difficulty of problems on LeetCode",
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problemset/all/",
        "https://leetcode.com/problems/*"
      ],
      "js": ["index.js"]
    }
  ],
  "icons": { "128": "icon_128.png" },
  "manifest_version": 2
}
